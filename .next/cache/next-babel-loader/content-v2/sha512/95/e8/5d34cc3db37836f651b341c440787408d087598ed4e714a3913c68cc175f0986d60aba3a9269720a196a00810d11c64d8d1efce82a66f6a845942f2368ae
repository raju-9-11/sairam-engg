 descError, setDecsError ] = useState('');\n\n    const router = useRouter();\n\n    const handleFieldChange = (val) => {\n        setField(val);\n    }\n    \n    const handleDescriptionChange = (val) => {\n        setDescription(val);\n    }\n\n    const handleImageChange = (val) => {\n        setImage(val)\n    }\n\n    const handleClickOpen = () => {\n      setOpen(true);\n    };\n\n    const handleClose = () => {\n        setOpen(false);\n    };\n\n    const handleSave = () => {\n      setNameError(field.length<4?'Name should be atleast 4 characters long':'');\n      setDecsError(description.length<2?'Name should be atleast 3 characters long':'');\n      if(field.length<4 && description.length<2){\n        return;\n      }\n      firestore\n        .collection('fields')\n        .doc(field.split(\" \").join(\"\").toLowerCase())\n        .set( {\n          name: field,\n          description: description,\n          image: image\n        })\n        .then((response) =>{\n          enqueueSnackbar(\"Field added\", { variant: 'success'})\n          // router.reload()\n          setDescription('');\n          setField('');\n          setImage('');\n          setOpen(false);\n        })\n        .catch((err) => {\n          console.log(err)\n          enqueueSnackbar(\"An error occured\", { variant: 'error'})\n        })\n\n  };\n\n  return (\n    <div >\n      <Button size = \"small\" onClick={handleClickOpen}>\n        Add Field\n      </Button>\n      <Dialog open={open} onClose={handleClose} aria-labelledby=\"form-dialog-title\">\n        <DialogTitle id=\"form-dialog-title\">Add Field</DialogTitle>\n        <DialogContent  className={classes.root}>\n          <DialogContentText>\n            To Add or modify field , please enter the required details.\n          </DialogContentText>\n          <Grid container spacing={5}>\n            <Grid item xs={12} sm={6}>\n            <TextField\n                autoFocus\n                required\n                variant=