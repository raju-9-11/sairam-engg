nc'\nimport { useSnackbar } from 'notistack';\nimport { useAuth } from '../lib/auth'\nimport { useRouter } from 'next/router'\n\n\nconst useStyles = makeStyles({\n  root: {\n      marginTop:'12vh'\n  },\n  form:{\n    padding:'7vh'\n  },\n  button: {\n    marginTop: '3vh'\n  },\n  box:{\n    padding: '10vh',\n    paddingTop: '25vh'\n  },\n})\n\nexport default function SignUp() {\n\n  const { user } = useAuth();\n  const classes = useStyles();\n  const { enqueueSnackbar } = useSnackbar();\n  const router = useRouter();\n\n  const [ email, setEmail ] = useState('');\n  const [ password, setPassword ] = useState('');\n  const [ firstName, setFirstName ] = useState('');\n  const [ lastName, setLastName ] = useState('');\n  const [ final, setFinal ] = useState('');\n  const [ emailError, setEmailError ] = useState('');\n  const [ passwordError, setPasswordError ] = useState('');\n  const { signupWithEmail } = useAuth();\n\n  useEffect(()=> {\n    if(user){\n      router.push('/dashboard/1')\n    }\n  },[user])\n\n\n\n  const handleSignUp = (e) => {\n    e.preventDefault();\n      setPasswordError(checkPassword(password,final));\n      setEmailError(checkEmail(email));\n      if(emailError==='' && passwordError===''){\n        try{\n          signupWithEmail(email, password, firstName, 0, lastName);\n        }\n        catch(E){\n          console.log(e);\n          enqueueSnackbar('Register error try again!',{variant:'error'});\n        }\n        if(user){\n          enqueueSnackbar('Registration Successful!',{variant:'success'});\n        }\n        return;\n      }\n    \n  }\n\n\n\n  return (\n    <Container component=\"main\" maxWidth=\"sm\" className={classes.root}>\n      <CssBaseline />\n      <Box className={classes.form}>\n        <Avatar sx={{ m: 1, bgcolor: 'secondary.main' }}>\n          <LockOutlinedIcon />\n        </Avatar>\n        <Typography component=\"h1\" variant=\"h5\" >\n          Sign up\n        </Typography>\n        <Box\n          className={classes.button}\n          component=\"form\"\n          noValidate\n          sx={{\n            width: '100%', // Fix IE11 issue.\n            mt: 3,\n          }}\n        >\n          <Grid container spacing={5}>\n            <Grid item xs={12} sm={6}>\n              <TextField\n                autoComplete=\"fname\"\n                name=\"firstName\"\n                required\n                fullWidth\n                value={firstName}\n                onChange={(Event)=>setFirstName(Event.target.value)}\n                id=\"firstName\"\n                label=\"First Name\"\n                variant=\"outlined\"\n                autoFocus\n              />\n            </Grid>\n            <Grid item xs={12} sm={6}>\n              <TextField\n                required\n                fullWidth\n                id=\"lastName\"\n                value={lastName}\n                onChange={(Event)=>setLastName(Event.target.value)}\n                label=\"Last Name\"\n                name=\"lastName\"\n                variant=\"outlined\"\n                autoComplete=\"lname\"\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <TextField\n                required\n                fullWidth\n                value={email}\n                onChange={(Event)=>setEmail(Event.target.value)}\n                id=\"email\"\n                error={emailError!=''}\n                helperText={emailError===''? '' : emailError}\n                label=\"Email Address\"\n                name=\"email\"\n                variant=\"outlined\"\n                autoComplete=\"email\"\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <TextField\n                required\n                fullWidth\n                value={password}\n                onChange={(Event)=>setPassword(Event.target.value)}\n                error={passwordError!=''}\n                helperText={passwordError===''?'': passwordError}\n                name=\"password\"\n                label=\"Password\"\n                type=\"password\"\n                id=\"password\"\n                variant=\"outlined\"\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <TextField\n                required\n                fullWidth\n                value={final}\n                onChange={(Event)=>setFinal(Event.target.value)}\n                error={passwordError!=''}\n                helperText={passwordError===''?'': passwordError}\n                name=\"cpassword\"\n                label=\"Confirm Password\"\n                type=\"password\"\n                id=\"cpassword\"\n                variant=\"outlined\"\n              />\n            </Grid>\n            {/* <Grid item xs={12}>\n              <FormControlLabel\n                control={<Checkbox value=\"allowExtraEmails\" color=\"primary\" />}\n                label=\"I want to receive inspiration, marketing promotions and updates via email.\"\n              />\n            </Grid> */}\n          </Grid>\n          <Button type=\"submit\" onClick={handleSignUp} fullWidth variant=\"contained\" sx={{ mt: 3, mb: 2 }} className={classes.button}>\n            Sign Up\n          </Button>\n          <Grid container justify=\"flex-end\" className={classes.button}>\n            <Grid item>\n              <Link href=\"/signin\" variant=\"body2\">\n                Already have an account? Sign in\n              </Link>\n            </Grid>\n          </Grid>\n        </Box>\n      </Box>\n      <Copyright sx={{ mt: 5 }} />\n    </Container>\n  );\n}\n"]},"metadata":{},"sourceType":"module"}